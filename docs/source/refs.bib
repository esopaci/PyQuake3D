@article{Devoti2011,
	abstract = {We present the velocity field in Italy derived from over 300 continuous GPS stations operated in the 1998\textendash{}2009 time span. The GPS network maps the whole country with a mean inter-site distance of about 50km and provides a valuable source of data to study the ongoing deformation processes in the central Mediterranean. The estimated horizontal and vertical velocity fields show major significant features and also less known second-order kinematic features. A general uplift characterizes the whole Apennines and Alpine belts that follow the topographic ridge, whereas the Po Plain shows a gradually increasing subsidence from west to east. The Apennines belt displays a distinctive extension (50\textendash{}80 10-9yr-1) while compressive tectonic regimes characterize northern Sicily, eastern Alps and the northeast front of the northern Apennines (25\textendash{}50 10-9yr-1). Second-order deformation patterns, on large scale wavelength (\textasciitilde{}100km) have been detected on the accretionary prism of central and southern Apennines that are highly correlated with other geophysical data (Vp anomalies, seismic anisotropy, etc.) and related to deep rooted sections (70\textendash{}100km), marked by different subduction regimes. Apparently at this scale-length the observed deformations are governed by the lithosphere as a whole. We interpret these deformations as a result of different subduction mechanisms, such as variations of the subduction rollback velocity affecting different segments of the subduction zone and/or to mantle flows in proximity of the slab edges. Further south, in central-southern Sicily, we detect a contraction of (-1.1$\pm$0.2) mm/yr that probably accommodates part of the Africa\textendash{}Eurasia convergence on the outer thrust front of the Apennines\textendash{}Maghrebides belt. This hypothesis agrees with an independent analysis of the seismicity associated to the Sicilian Basal Thrust, thought to be still active. The ITRF2005 estimates of the new GPS velocity field are available also in SINEX format as supplementary file S1.},
	author = {Devoti, Roberto and Esposito, Alessandra and Pietrantonio, Grazia and Pisani, Anna Rita and Riguzzi, Federica},
	date-added = {2018-11-20 08:16:14 +0000},
	date-modified = {2018-11-20 08:16:14 +0000},
	doi = {10.1016/j.epsl.2011.09.034},
	file = {C:\\Users\\mario\\OneDrive\\Zotero\\storage\\6F6CRQ34\\Devoti et al. - 2011 - Evidence of large scale deformation patterns from .pdf;C:\\Users\\mario\\OneDrive\\Zotero\\storage\\ZLM3T3S9\\S0012821X11005541.html},
	issn = {0012-821X},
	journal = {Earth and Planetary Science Letters},
	keywords = {Apennines,Alps,Adria,GPS velocity field,plate kinematics},
	month = nov,
	number = {3},
	pages = {230-241},
	title = {Evidence of Large Scale Deformation Patterns from {{GPS}} Data in the {{Italian}} Subduction Boundary},
	volume = {311},
	year = {2011},
	bdsk-url-1 = {http://dx.doi.org/10.1016/j.epsl.2011.09.034}}

@article{Doglioni1996,
	abstract = {The Apennines comprise a Neogen\textemdash{}Quaternary accretionary prism that shows several anomalies with respect to classic alpine-type mountain belts, namely (i) low elevation, (ii) a shallow new Moho below the core of the belt, (iii) high heat flow in the internal parts, (iv) mainly sedimentary cover involved in the prism, (v) a deep foredeep and (vi) a fully developed back-arc basin. The suction exerted by a relatively eastward migrating mantle can determine the eastward retreat of the subduction zone and an asthenospheric wedging at the retreating subduction hinge. Heat flow, geochemical and seismological data support the presence of a hot mantle wedge underlying the western side of the Apenninic accretionary prism. A thermal model of the belt with foreland dipping isotherms fits with deepening of the seismicity toward the east. Mantle volatiles signatures are also widespread in springs along the Apennines.},
	author = {Doglioni, C. and Harabaglia, P. and Martinelli, G. and Mongelli, F. and Zito, G.},
	date-added = {2018-11-20 08:15:44 +0000},
